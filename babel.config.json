{
    "presets": ["@babel/env", ["@babel/react", { "runtime": "automatic" }], "@babel/typescript"],
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "@components": "./src/components",
                    "@images": "./src/assets/images",
                    "@icons": "./src/assets/icons",
                    "@pages": "./src/pages"
                }
            }
        ]
    ]
}
